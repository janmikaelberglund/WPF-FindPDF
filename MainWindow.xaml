<Window x:Class="WPF_FindPDF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_FindPDF"
        mc:Ignorable="d"
        Title="MainWindow" Height="750" Width="550">
    <Grid Margin="0,0,0,0">
        <TextBox x:Name="PathInput" HorizontalAlignment="Left" Height="30" Width="300" Margin="25,25,0,0" TextWrapping="Wrap" VerticalAlignment="Top" TextChanged="TextBox_TextChanged"/>
        <Button x:Name="Open" Content="Öppna..." HorizontalAlignment="Left" Height="30" VerticalAlignment="Top" Width="60" Click="Button_Open_Click" Margin="330,25,0,629"/>
        <Button x:Name="Run" Content="Kör" HorizontalAlignment="Left" Height="30" Width="60" VerticalAlignment="Top" Margin="395,25,0,629" Click="Button_Run_Click"/>
        <Button x:Name="Save" Content="Spara" HorizontalAlignment="Left" Height="30" Width="80" VerticalAlignment="Top" Click="Button_Save_Click" Margin="395,625,0,29"/>
        <ListBox x:Name="Output_Listbox" HorizontalAlignment="Left" Height="600" Margin="25,70,0,0" VerticalAlignment="Top" Width="300"/>
        <Label x:Name="SaveResultLabel" Content="" HorizontalAlignment="Left" Height="25" Margin="395,670,0,0" VerticalAlignment="Top" Width="80"/>
    </Grid>
</Window>
